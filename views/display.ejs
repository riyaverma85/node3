<hr color="black" size="3">
   <h2>Data dispaly page!!!</h2>
    <table border="1" width="500px" height="300px">
        <tr>
            <th>Name</th>
            <th>Subject</th>
            <th>City</th>
            <th>Fees</th>
        </tr>
    
   <% Data.forEach((key)=>{ %>
        <tr>
            <td> <%= key.name %></td>
            <td> <%= key.subject %></td>
            <td> <%= key.city %></td>
            <td> <%= key.fees %></td>
        </tr>
      <% }) %>

    </table>

</body>
</html>